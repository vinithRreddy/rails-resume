Education
<%= form_with(model: @user,url: wizard_path, local: true)  do |form| %>

<div class='nested-fields'>
  <div class='form-group'>
    <%= form.hidden_field :_destroy %>

    <div class="field">
    <%= form.label :institute_name %>
    <%= form.text_field :institute_name %>
    </div>

    <div class="field">
    <%= form.label :qualification %>
    <%= form.text_field :qualification %>
    </div>

    <div class="field">
    <%= form.label :specification %>
    <%= form.text_field :specification %>
    </div>

    <div class="field">
    <%= form.label :start_date %>
    <%= form.date_select :start_date %>
    </div>

    <div class="field">
    <%= form.label :end_date %>
    <%= form.date_select :end_date %>
    </div>

    <div class="field">
    <%= form.label :marks %>
    <%= form.text_field :marks %>
    </div>

    <small>
      <%= link_to "Remove", "#", data: { action: "click->nested-form#remove_association" } %>
    </small>
  </div>
</div>
<div class="actions">
<%= form.submit "Continue" %>
</div>

<% end %>